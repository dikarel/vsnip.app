{
  "name": "v-snip",
  "description": "Visual snippet copy-paste tool",
  "version": "0.0.0",
  "private": true,
  "main": "app/main.js",
  "scripts": {
    "start": "electron app/main",
    "lint": "standard && flow; test $? -eq 0 -o $? -eq 2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "electron-prebuilt-compile": "^1.4.6",
    "eslint": "^3.9.1",
    "eslint-plugin-flowtype": "^2.25.0",
    "flow-bin": "^0.34.0",
    "standard": "^8.5.0"
  },
  "dependencies": {
    "highland": "^2.10.1",
    "vue": "^2.0.5"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "babel": {
    "plugins": [
      "syntax-flow",
      "transform-flow-strip-types"
    ]
  }
}
